{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "MApracticelog",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string",
            "pattern": "^[a-fA-F0-9]{24}$"
        },
        "team_id": {
            "type": "string",
            "pattern": "^[a-fA-F0-9]{24}$"
        },
        "log": {
            "type": "object",
            "additionalProperties": false,
            "patternProperties": {
                "^[a-zA-Z0-9_.-]+$": {
                    "type": "integer"
                }
            }
        }
    },
    "required": [
        "_id",
        "team_id"
    ]
}